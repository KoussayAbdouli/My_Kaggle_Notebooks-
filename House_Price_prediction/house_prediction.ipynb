    "def Diviseur(N):\n",
    "    list_div = []\n",
    "    for num in range(1,N+1):\n",
    "        if (N%num == 0):\n",
    "            list_div.append(num)\n",
    "    for i in list_div:\n",
    "        print(i) \n",
    "     \n",
    "\n"
   ]
